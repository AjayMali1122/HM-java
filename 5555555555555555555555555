//Q5. Write the code.

public class Student2                          //class 1
{
	private int rollno;
	private String name;
     	                                 //setter/getter;

        public int getRollno()
       {
        return rollno;

      }
      public void setRollno(int rollno )
       {
        this.rollno= rollno;

      }

       public String  getName()
       {
        return name;

      }
      public void setName(String name )
       {
        this.name= name;

      }

}

public class Test2                                      //class 2
{
	public void display(Student s)
	{
           System.out.println(s.getRollno());
           System.out.println(s.getName());


		                     //get student data here
	}
	public static void main(String[] args) 
	{
		Student s=new Student();
                 s.setRollno(102);
                 s.setName("ajay");

		//Set student data here	
        Test2 t = new Test2();
        t.display(s); // Calling display method 

	}
}
